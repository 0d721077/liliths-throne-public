<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!-- Look in the mods folder for a commented version of this. -->
<outfit>
	<coreAttributes>
		<name><![CDATA[masculine mugger]]></name>
		
		<description><![CDATA[Dark, concealing clothing, with the possibility of a weapon or two...]]></description>
		
		<femininity>MASCULINE</femininity>
		
		<outfitTypes>
		  <type>MUGGER</type>
		</outfitTypes>
		
		<acceptableLegConfigurations/>
		
		<conditional/>
		
		<weight>100</weight>
	</coreAttributes>
	
	<generationAttributes>
		<clothingConditional1><![CDATA[RND.​nextInt(100)<=75]]></clothingConditional1>
		<clothingConditional2><![CDATA[RND.​nextInt(100)<=50]]></clothingConditional2>
		<clothingConditional3><![CDATA[RND.​nextInt(100)<=25]]></clothingConditional3>
		
		<presetColourGroup1>
			<randomColour>CLOTHING_BLACK</randomColour>
			<randomColour>CLOTHING_GREEN_DRAB</randomColour>
			<randomColour>CLOTHING_OLIVE</randomColour>
			<randomColour>CLOTHING_BROWN_VERY_DARK</randomColour>
		</presetColourGroup1>
		
		<presetColourGroup2 values="ALL_METAL"/>
		
		<presetColourGroup3>
			<randomColour>CLOTHING_BLACK</randomColour>
			<randomColour>CLOTHING_WHITE</randomColour>
			<randomColour>CLOTHING_BLUE_DARK</randomColour>
			<randomColour>CLOTHING_BLUE_LIGHT</randomColour>
		</presetColourGroup3>
		
        <mainWeapons>
			<weapon>
				<conditional><![CDATA[clothingConditional2]]></conditional>
				<type>innoxia_pipe_pipe</type>
				<damageTypes>
					<damage>PHYSICAL</damage>
				</damageTypes>
				<primaryColours/>
				<secondaryColours/>
			</weapon>
        </mainWeapons>
		
        <offhandWeapons/>
		
		<guaranteedClothingEquips/>
		
		<genericClothingType/>
		
		<clothingType>
			<conditional><![CDATA[npc.getLegConfiguration()==LEG_CONFIGURATION_BIPEDAL]]></conditional>
			<types>
				<type>LEG_CARGO_TROUSERS</type>
				<type>innoxia_leg_distressed_jeans</type>
				<type>LEG_JEANS</type>
			</types>
			<primaryColours>
				<colour>CLOTHING_BLACK</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>TORSO_TSHIRT</type>
				<type>TORSO_SHORT_SLEEVE_SHIRT</type>
			</types>
			<primaryColours>
				<colour>presetColourGroup1</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>TORSO_OVER_HOODIE</type>
				<type>TORSO_SHORT_SLEEVE_SHIRT</type>
			</types>
			<primaryColours>
				<colour>presetColourGroup1</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional><![CDATA[clothingConditional3]]></conditional>
			<types>
				<type>HAND_WRAPS</type>
				<type>HAND_FINGERLESS_GLOVES</type>
			</types>
			<primaryColours>
				<colour>presetColourGroup1</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional><![CDATA[clothingConditional2]]></conditional>
			<types>
				<type>MOUTH_BANDANA</type>
			</types>
			<primaryColours>
				<colour>presetColourGroup1</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>GROIN_JOCKSTRAP</type>
				<type>GROIN_BRIEFS</type>
				<type>GROIN_BOXERS</type>
			</types>
			<primaryColours>
				<colour>presetColourGroup3</colour>
			</primaryColours>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>SOCK_SOCKS</type>
			</types>
			<primaryColours values="BLACK_OR_WHITE"/>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>FOOT_WORK_BOOTS</type>
			</types>
			<primaryColours/>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
		<clothingType>
			<conditional/>
			<types>
				<type>FOOT_TRAINERS</type>
			</types>
			<primaryColours/>
			<secondaryColours/>
			<tertiaryColours/>
		</clothingType>
		
	</generationAttributes>

</outfit>